<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]>      <html class="no-js"> <!--<![endif]-->
<html>

<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="description" content="" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <link rel="stylesheet" href="/public/stylesheets/overview.css" />
  <link rel="stylesheet" href="/public/stylesheets/charts.min.css" />
</head>

<body>
  <div class="background">
    <%- include('partials/header.ejs') %>
    <div class="subheader">
      <h2>Overview</h2>
    </div>
    <div class="container">
      <div class="content">

        <table class="charts-css column show-heading multiple" id="my-chart">
          <caption> Descriptive Chart Heading </caption>
          <tr>
            <td style="--size: calc(25 / 100)"> 1 </td>
            <td style="--size: calc(50 / 100)"> 2 </td>
            <td style="--size: calc(75 / 100)"> 3 </td>
            <td style="--size: calc(100 / 100)"> 4 </td>
          </tr>
        </table>

        <table class="charts-css area" id="my-chart">
          <tbody>
            <tr>
              <td style="--start: 0.2; --size: 0.4"> <span class="data"> $ 40K </span> </td>
            </tr>
            <tr>
              <td style="--start: 0.4; --size: 0.8"> <span class="data"> $ 80K </span> </td>
            </tr>
            <tr>
              <td style="--start: 0.8; --size: 0.6"> <span class="data"> $ 60K </span> </td>
            </tr>
            <tr>
              <td style="--start: 0.6; --size: 1.0"> <span class="data"> $ 100K </span> </td>
            </tr>
            <tr>
              <td style="--start: 1.0; --size: 0.3"> <span class="data"> $ 30K </span> </td>
            </tr>
          </tbody>
        </table>

        <table class="charts-css line" id="my-chart">
          <tbody>
            <tr>
              <td style="--start: 0.0; --size: 0.4"> <span class="data"> $ 40K </span> </td>
            </tr>
            <tr>
              <td style="--start: 0.4; --size: 0.2"> <span class="data"> $ 20K </span> </td>
            </tr>
            <tr>
              <td style="--start: 0.2; --size: 0.6"> <span class="data"> $ 60K </span> </td>
            </tr>
            <tr>
              <td style="--start: 0.6; --size: 0.4"> <span class="data"> $ 40K </span> </td>
            </tr>
            <tr>
              <td style="--start: 0.4; --size: 0.8"> <span class="data"> $ 80K </span> </td>
            </tr>
            <tr>
              <td style="--start: 0.8; --size: 0.6"> <span class="data"> $ 60K </span> </td>
            </tr>
            <tr>
              <td style="--start: 0.6; --size: 1.0"> <span class="data"> $ 100K </span> </td>
            </tr>
          </tbody>
        </table>
      </div>
      <%- include('partials/weights sidebar.ejs') %>
    </div>
  </div>
  <script>
  </script>
</body>

</html>